{
    "@context": {
        "@version": 1.1,
        "ls": "https://lschema.org/",
        "Value": {
            "@id": "ls:Value",
            "@context": {}
        },
        "Object": {
            "@id": "ls:Object",
            "@context": {
                "attributes": {
                    "@id": "ls:Object#attributes",
                    "@container": "@id"
                },
                "attributeList": {
                    "@id": "ls:Object#attributeList",
                    "@container": "@list"
                }
            }
        },
        "Array": {
            "@id": "ls:Array",
            "@context": {
                "items": {
                    "@id": "ls:Array#items"
                }
            }
        },
        "Reference": {
            "@id": "ls:Reference",
            "@context": {
                "reference": {
                    "@id":"ls:Reference#reference",
                    "@type": "@id"
                }
            }
        },
        "Composite": {
            "@id": "ls:Composite",
            "@context": {
                "allOf": {
                    "@id": "ls:Composite#allOf",
                    "@container": "@list"
                }
            }
        },
        "Polymorphic": {
            "@id": "ls:Polymorphic",
            "@context": {
                "oneOf": {
                    "@id": "ls:Polymorphic#oneOf",
                    "@container": "@list"
                }
            }
        },
        "Overlay": {
            "@id": "ls:Overlay",
            "@context": {
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                },
                "attributes": {
                    "@id": "ls:Object#attributes",
                    "@container": "@id"
                },
                "attributeList": {
                    "@id": "ls:Object#attributeList",
                    "@container": "@list"
                }
            }
        },
        "Schema": {
            "@id": "ls:Schema",
            "@context": {
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                },
                "attributes": {
                    "@id": "ls:Object#attributes",
                    "@container": "@id"
                },
                "attributeList": {
                    "@id": "ls:Object#attributeList",
                    "@container": "@list"
                }
             }
        },

        "SchemaManifest": {
            "@id": "ls:SchemaManifest",
            "@context": {
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                },
                "bundle": {
                    "@id": "ls:SchemaManifest#bundle",
                    "@type": "@id"
                },
                "schema": {
                    "@id": "ls:SchemaManifest#schema",
                    "@type": "@id"
                },
                "overlays": {
                    "@id": "ls:SchemaManifest#overlays",
                    "@type": "@id",
                    "@container": "@list"
                }
            }
        },

        "description": {
           "@id": "ls:description"
        },

        "validation#required": {
            "@id": "ls:validation#required"
        },
        "validation#pattern": {
            "@id": "ls:validation#pattern"
        },
        "validation#format": {
            "@id": "ls:validation#format"
        },
        "validation#enumeration": {
            "#id": "ls:validation#enumeration"
        }
          
    }
}
