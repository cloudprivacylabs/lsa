{
    "@context": {
        "@version": 1.1,
        
        "ls": "https://lschema.org/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "json": "https://json.org#",
        
        "Attribute": "ls:Attribute",
        "DocumentNode":"ls:DocumentNode",
        "Value": "ls:Value",
        "Object": {
            "@id": "ls:Object",
            "@context": {
                "attributes": {
                    "@id": "ls:Object/attributes",
                    "@container": "@id"
                },
                "attributeList": {
                    "@id": "ls:Object/attributeList",
                    "@container": "@list"
                }
            }
        },
        "Array": {
            "@id": "ls:Array",
            "@context": {
                "arrayElements":  "ls:Array/elements"
            }
        },
        "Reference": {
            "@id": "ls:Reference",
            "@context": {
                "reference": {
                    "@id":"ls:Reference/ref",
                    "@type": "@id"
                },
                "fields": "ls:Reference/fields",
                "targetFields": "ls:Reference/targetFields",
                "label": "ls:Reference/label",
                "direction": "ls:Reference/direction",
                "multi": "ls:Reference/multi"
            }
        },
        "Composite": {
            "@id": "ls:Composite",
            "@context": {
                "allOf": {
                    "@id": "ls:Composite/allOf",
                    "@container": "@list"
                }
            }
        },
        "Polymorphic": {
            "@id": "ls:Polymorphic",
            "@context": {
                "oneOf": {
                    "@id": "ls:Polymorphic/oneOf",
                    "@container": "@list"
                }
            }
        },
        
        "layer":  "ls:layer",
        "Overlay": {
            "@id": "ls:Overlay",
            "@context": {
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                }
            }
        },
        "Schema": {
            "@id": "ls:Schema",
            "@context": {
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                }
             }
        },

        "SchemaVariant": {
            "@id": "ls:SchemaVariant",
            "@context": {
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                },
                "bundle": {
                    "@id": "ls:bundle",
                    "@type": "@id"
                },
                "schema": {
                    "@id": "ls:SchemaVariant/schema",
                    "@type": "@id"
                },
                "overlays": {
                    "@id": "ls:SchemaVariant/overlays",
                    "@type": "@id",
                    "@container": "@list"
                }
            }
        },

        "lookupTable": {
            "@id": "ls:lookupTable",
            "@context": {
                "id": "@id",
                "ref": "@id",
                "elements": {
                    "@id": "ls:lookupTable/elements",
                    "@container": "@list",
                    "@context": {
                        "options": "ls:lookupTable/element/options",
                        "caseSensitive": "ls:lookupTable/element/caseSensitive",
                        "value": "ls:lookupTable/element/value",
                        "error": "ls:lookupTable/element/error"
                    }
                }
            }
        },


        "entityId": "ls:entityId",

        "characterEncoding":"ls:characterEncoding",
        "description":"ls:description",
        "attributeName": "ls:attributeName",
        "attributeIndex": "ls:attributeIndex",
        "attributeValue": "ls:attributeValue",
        "label": "ls:label",

        "defaultValue":"ls:defaultValue",

        "required":"ls:validation/required",
        "pattern":"ls:validation/pattern",
        "jsonFormat": "ls:validation/jsonFormat",
        "enumeration": "ls:validation/enumeration",

        "goTimeFormat": "ls:goTimeFormat",
        "momentTimeFormat": "ls:momentTimeFormat",

        "has": "ls:has",
        "instanceOf":"ls:instanceOf",
        "asPropertyOf" : "ls:asPropertyOf",
        "asProperty": "ls:asProperty"

    }
}
