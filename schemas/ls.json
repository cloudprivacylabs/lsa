{
    "@context": {
        "@version": 1.1,
        "@protected": true,
        
        "ls": "https://lschema.org/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "json": "https://json.org#",
        
        "Attribute": "ls:Attribute",
        "DocumentNode":"ls:DocumentNode",
        "Value": "ls:Value",
        "Object": {
            "@id": "ls:Object",
            "@context": {
                "@protected": true,
                "attributes": {
                    "@id": "ls:Object/attributes",
                    "@container": "@id"
                },
                "attributeList": {
                    "@id": "ls:Object/attributeList",
                    "@container": "@list"
                }
            }
        },
        "Array": {
            "@id": "ls:Array",
            "@context": {
                "@protected": true,
                "arrayElements":  "ls:Array/elements"
            }
        },
        "Reference": {
            "@id": "ls:Reference",
            "@context": {
                "@protected": true,
                "reference": {
                    "@id":"ls:Reference/ref",
                    "@type": "@id"
                }
            }
        },
        "Composite": {
            "@id": "ls:Composite",
            "@context": {
                "@protected": true,
                "allOf": {
                    "@id": "ls:Composite/allOf",
                    "@container": "@list"
                }
            }
        },
        "Polymorphic": {
            "@id": "ls:Polymorphic",
            "@context": {
                "@protected": true,
                "oneOf": {
                    "@id": "ls:Polymorphic/oneOf",
                    "@container": "@list"
                }
            }
        },
        
        "layer":  "ls:layer",
        "Overlay": {
            "@id": "ls:Overlay",
            "@context": {
                "@protected": true,
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                }
            }
        },
        "Schema": {
            "@id": "ls:Schema",
            "@context": {
                "@protected": true,
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                }
             }
        },

        "SchemaManifest": {
            "@id": "ls:SchemaManifest",
            "@context": {
                "@protected": true,
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                },
                "bundle": {
                    "@id": "ls:bundle",
                    "@type": "@id"
                },
                "schema": {
                    "@id": "ls:SchemaManifest/schema",
                    "@type": "@id"
                },
                "overlays": {
                    "@id": "ls:SchemaManifest/overlays",
                    "@type": "@id",
                    "@container": "@list"
                }
            }
        },

        "characterEncoding":"ls:characterEncoding",
        "description":"ls:description",
        "attributeName": "ls:attributeName",
        "attributeIndex": "ls:attributeIndex",
        "attributeValue": "ls:attributeValue",
        "label": "ls:label",

        "defaultValue":"ls:defaultValue",

        "required":"ls:validation/required",
        "pattern":"ls:validation/pattern",
        "jsonFormat": "ls:validation/jsonFormat",
        "enumeration": "ls:validation/enumeration",

        "has": "ls:data/has",
        "instanceOf":"ls:data/instanceOf",
        "propertyOf": {
            "@id": "ls:propertyOf",
            "@type": "@id"
        },
        "asProperty": "ls:asProperty"
    }
}
