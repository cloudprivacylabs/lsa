{
    "@context": {
        "@version": 1.1,
        "@protected": true,
        "ls": "https://lschema.org/",
        "Attribute": {
            "@id": "ls:Attribute"
        },
        "DocumentNode": {
            "@id": "ls:DocumentNode"
        },
        "Value": {
            "@id": "ls:Value",
            "@context": {}
        },
        "Object": {
            "@id": "ls:Object",
            "@context": {
                "@protected": true,
                "attributes": {
                    "@id": "ls:Object#attributes",
                    "@container": "@id"
                },
                "attributeList": {
                    "@id": "ls:Object#attributeList",
                    "@container": "@list"
                }
            }
        },
        "Array": {
            "@id": "ls:Array",
            "@context": {
                "@protected": true,
                "items": {
                    "@id": "ls:Array#items"
                }
            }
        },
        "Reference": {
            "@id": "ls:Reference",
            "@context": {
                "@protected": true,
                "reference": {
                    "@id":"ls:Reference#reference",
                    "@type": "@id"
                }
            }
        },
        "Composite": {
            "@id": "ls:Composite",
            "@context": {
                "@protected": true,
                "allOf": {
                    "@id": "ls:Composite#allOf",
                    "@container": "@list"
                }
            }
        },
        "Polymorphic": {
            "@id": "ls:Polymorphic",
            "@context": {
                "@protected": true,
                "oneOf": {
                    "@id": "ls:Polymorphic#oneOf",
                    "@container": "@list"
                }
            }
        },
        
        "layer": {
            "@id": "ls:layer",
            "@context": {
                "attributes": {
                    "@id": "ls:Object#attributes",
                    "@container": "@id"
                },
                "attributeList": {
                    "@id": "ls:Object#attributeList",
                    "@container": "@list"
                }
            }
        },
        
        "Overlay": {
            "@id": "ls:Overlay",
            "@context": {
                "@protected": true,
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                }
            }
        },
        "Schema": {
            "@id": "ls:Schema",
            "@context": {
                "@protected": true,
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                }
             }
        },

        "SchemaManifest": {
            "@id": "ls:SchemaManifest",
            "@context": {
                "@protected": true,
                "targetType": {
                    "@id": "ls:targetType",
                    "@type": "@id"
                },
                "bundle": {
                    "@id": "ls:SchemaManifest#bundle",
                    "@type": "@id"
                },
                "schema": {
                    "@id": "ls:SchemaManifest#schema",
                    "@type": "@id"
                },
                "overlays": {
                    "@id": "ls:SchemaManifest#overlays",
                    "@type": "@id",
                    "@container": "@list"
                }
            }
        },

        "characterEncoding": {
           "@id": "ls:characterEncoding"
        },
        "description": {
           "@id": "ls:description"
        },
        "attributeName": {
            "@id": "ls:attributeName"
        },
        "attributeIndex": {
            "@id": "ls:attributeIndex"
        },
        

        "defaultValue": {
            "@id": "ls:defaultValue"
        },
        
        "validation#required": {
            "@id": "ls:validation#required"
        },
        "validation#pattern": {
            "@id": "ls:validation#pattern"
        },
        "validation#format": {
            "@id": "ls:validation#format"
        },
        "validation#enumeration": {
            "@id": "ls:validation#enumeration"
        },

        "dataObjectAttributes": {
            "@id": "ls:data/object#attributes"
        },
        "dataArrayElements": {
            "@id": "ls:data/array#elements"
        },
        "dataInstanceOf": {
            "@id": "ls:data#instanceOf"
        }
          
    }
}
