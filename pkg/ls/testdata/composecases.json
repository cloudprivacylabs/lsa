[
    {
        "name": "BasicCompose",
        "base": {
            "@context": {
                "ls":"https://lschema.org/"
            },
            "@type":"ls:Schema",
            "@id": "http://testschema",
            "https://lschema.org/layer": {
                "@type": ["https://lschema.org/Object",
                          "https://lschema.org/valueType"],
                "ls:Object/attributes": [
                    {
                        "@id":  "attr1",
                        "@type": "ls:Value"
                    },
                    {
                        "@id":  "attr2" ,
                        "@type": "ls:Value",
                        "ls:privacy": [
                            {
                                "@value": "flg1"
                            }
                        ]
                    },
                    {
                        "@id":"attr3",
                        "@type": "ls:Value",
                        "ls:privacy": [
                            {"@value": "flg2"},
                            {"@value": "flg3"}
                        ]
                    }
                ]
            }
        },
        "overlays": [
            {
                "@context": {
                    "ls":"https://lschema.org/"
                },
                "@type":"ls:Overlay",
                "@id": "http://testOvl",
                "https://lschema.org/layer": {
                    "@type": ["https://lschema.org/Object",
                              "https://lschema.org/valueType"],
                    "ls:Object/attributes": [
                        {
                            "@id":"attr1",
                            "@type": "ls:Value",
                            "http://someKey" : [
                                {"@value": "someValue"}
                            ]
                        },
                        {
                            "@id":"attr2",
                            "@type": "ls:Value",
                            "ls:privacy" : [
                                {"@value": "addFlg1"}
                            ]
                        },
                        {
                            "@id": "attr3",
                            "@type": "ls:Value",
                            "ls:privacy" : [
                                {"@value": "addFlg2"},
                                {"@value": "addFlg3"}
                            ]
                        }
                    ]
                }
            }
        ],
        "expected": [{
            "@id": "http://testschema",
            "@type":["https://lschema.org/Schema"],
            "https://lschema.org/layer": [{
                "@type": ["https://lschema.org/Attribute",
                          "https://lschema.org/Object",
                          "https://lschema.org/valueType"],
                "https://lschema.org/Object/attributes": [
                    {
                        "@id":  "attr1",
                        "@type": ["https://lschema.org/Attribute","https://lschema.org/Value"],
                        "https://lschema.org/attributeIndex": [{"@value":"0"}],
                        "http://someKey" : [
                            {"@value": "someValue"}
                        ]
                    },
                    {
                        "@id":  "attr2" ,
                        "@type": ["https://lschema.org/Attribute","https://lschema.org/Value"],
                        "https://lschema.org/attributeIndex": [{"@value":"1"}],
                        "https://lschema.org/privacy": [
                            {
                                "@value": "flg1"
                            },
                            {"@value": "addFlg1"}
                        ]
                    },
                    {
                        "@id":"attr3",
                        "@type": ["https://lschema.org/Attribute","https://lschema.org/Value"],
                        "https://lschema.org/attributeIndex": [{"@value":"2"}],
                        "https://lschema.org/privacy": [
                            {"@value": "flg2"},
                            {"@value": "flg3"},
                            {"@value": "addFlg2"},
                            {"@value": "addFlg3"}
                         ]
                    }
                ]
            }]
        }]
    },
    {
        "name": "RootAnnotations",
        "base": {
            "@context": {
                "ls":"https://lschema.org/"
            },
            "@type":"ls:Schema",
            "@id": "http://testschema",
            "https://lschema.org/layer": {
                "@type": ["https://lschema.org/Object",
                          "https://lschema.org/valueType"],
                "@id": "https://root",
                "ls:Object/attributes": [
                    {
                        "@id":  "attr1",
                        "@type": "ls:Value"
                    },
                    {
                        "@id":  "attr2" ,
                        "@type": "ls:Value",
                        "ls:privacy": [
                            {
                                "@value": "flg1"
                            }
                        ]
                    },
                    {
                        "@id":"attr3",
                        "@type": "ls:Value",
                        "ls:privacy": [
                            {"@value": "flg2"},
                            {"@value": "flg3"}
                        ]
                    }
                ]
            }
        },
        "overlays": [
            {
                "@context": {
                    "ls":"https://lschema.org/"
                },
                "@type":"ls:Overlay",
                "@id": "http://testOvl",
                "https://lschema.org/layer": {
                    "@type": ["https://lschema.org/Object",
                              "https://lschema.org/valueType"],
                    "@id": "https://root",
                    "https://annotation":"value",
                    "ls:Object/attributes": [
                        {
                            "@id":"attr1",
                            "@type": "ls:Value",
                            "http://someKey" : [
                                {"@value": "someValue"}
                            ]
                        },
                        {
                            "@id":"attr2",
                            "@type": "ls:Value",
                            "ls:privacy" : [
                                {"@value": "addFlg1"}
                            ]
                        },
                        {
                            "@id": "attr3",
                            "@type": "ls:Value",
                            "ls:privacy" : [
                                {"@value": "addFlg2"},
                                {"@value": "addFlg3"}
                            ]
                        }
                    ]
                }
            }
        ],
        "expected": [{
            "@id": "http://testschema",
            "@type":["https://lschema.org/Schema"],
            "https://lschema.org/layer": [{
                "@id": "https://root",
                "@type": ["https://lschema.org/Attribute",
                          "https://lschema.org/Object",
                          "https://lschema.org/valueType"],
                "https://annotation":[
                    {
                        "@value":"value"
                    }
                ],
                "https://lschema.org/Object/attributes": [
                    {
                        "@id":  "attr1",
                        "@type": ["https://lschema.org/Attribute","https://lschema.org/Value"],
                        "https://lschema.org/attributeIndex": [{"@value":"0"}],
                        "http://someKey" : [
                            {"@value": "someValue"}
                        ]
                    },
                    {
                        "@id":  "attr2" ,
                        "@type": ["https://lschema.org/Attribute","https://lschema.org/Value"],
                        "https://lschema.org/attributeIndex": [{"@value":"1"}],
                        "https://lschema.org/privacy": [
                            {
                                "@value": "flg1"
                            },
                            {"@value": "addFlg1"}
                        ]
                    },
                    {
                        "@id":"attr3",
                        "@type": ["https://lschema.org/Attribute","https://lschema.org/Value"],
                        "https://lschema.org/attributeIndex": [{"@value":"2"}],
                        "https://lschema.org/privacy": [
                            {"@value": "flg2"},
                            {"@value": "flg3"},
                            {"@value": "addFlg2"},
                            {"@value": "addFlg3"}
                         ]
                    }
                ]
            }]
        }]
    },

    {
        "name": "ArrayCompose",
        "base": {
            "@context": {
                "ls":"https://lschema.org/"
            },
            "@type":"ls:Schema",
            "@id": "http://testschema",
            "https://lschema.org/layer": {
                "@type": ["https://lschema.org/Object",
                          "https://lschema.org/valueType"],
                "ls:Object/attributes": [
                    {
                        "@id": "array",
                        "@type": "ls:Array",
                        "ls:Array/elements":  {
                            "@id": "http://items",
                            "@type": "ls:Value"
                        }
                    }
                ]
            }
        },
        "overlays": [
            {
                "@context": {
                    "ls":"https://lschema.org/"
                },
                "@type":"ls:Overlay",
                "@id": "http://testschema",
                "https://lschema.org/layer": {
                    "@type": ["https://lschema.org/Object",
                              "https://lschema.org/valueType"],
                    "ls:Object/attributes": [
                        {
                            "@id": "array",
                            "@type": "ls:Array",
                            "ls:Array/elements":  {
                                "@id": "http://items",
                                "@type": "ls:Value",
                                "http://annotation": "value"
                            }
                        }
                    ]
                }
            }
        ],
        "expected": [{
            "@type":["https://lschema.org/Schema"],
            "@id": "http://testschema",
            "https://lschema.org/layer": [{
                "@type": ["https://lschema.org/Attribute",
                          "https://lschema.org/Object",
                          "https://lschema.org/valueType"],
                "https://lschema.org/Object/attributes": [
                    {
                        "@id": "array",
                        "https://lschema.org/attributeIndex": [{"@value":"0"}],
                        "@type": ["https://lschema.org/Array","https://lschema.org/Attribute"],
                        "https://lschema.org/Array/elements":  [{
                            "@id": "http://items",
                            "@type": ["https://lschema.org/Attribute","https://lschema.org/Value"],
                            "http://annotation": [{"@value":"value"}]
                        }]
                    }
                ]
            }]
        }]
    },

    {
        "name": "PolymorphicCompose",
        "base": {
            "@context": {
                "ls":"https://lschema.org/"
            },
            "@type":"ls:Schema",
            "@id": "http://testschema",
            "https://lschema.org/layer": {
                "@type": ["https://lschema.org/Object",
                          "https://lschema.org/valueType"],
                "ls:Object/attributes": [
                    {
                        "@id": "attr1",
                        "@type": "ls:Polymorphic",
                        "ls:Polymorphic/oneOf": [
                            {
                                "@list": [
                                    {
                                        "@id": "id1",
                                        "@type": "ls:Value"
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        },
        "overlays": [
            {
                "@context": {
                    "ls":"https://lschema.org/"
                },
                "@type":"ls:Overlay",
                "@id": "http://testschema",
                "https://lschema.org/layer": {
                    "@type": ["https://lschema.org/Object",
                              "https://lschema.org/valueType"],
                    "ls:Object/attributes": [
                        {
                            "@id": "attr1",
                            "@type": "ls:Polynorphic",
                            "ls:Polymorphic/oneOf": [
                                {
                                    "@list": [
                                        {
                                            "@id": "id1",
                                            "@type": "ls:Value",
                                            "http://annotation": "value"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            }
        ],
        "expected": [{
            "@type":["https://lschema.org/Schema"],
            "@id": "http://testschema",
            "https://lschema.org/layer": [{
                "@type": ["https://lschema.org/Attribute",
                          "https://lschema.org/Object",
                          "https://lschema.org/valueType"],
                "https://lschema.org/Object/attributes": [
                    {
                        "@id": "attr1",
                        "@type": ["https://lschema.org/Attribute","https://lschema.org/Polymorphic"],
                        "https://lschema.org/attributeIndex": [{"@value":"0"}],
                        "https://lschema.org/Polymorphic/oneOf":  [
                            {
                                "@list": [{
                                    "@id": "id1",
                                    "@type": ["https://lschema.org/Attribute","https://lschema.org/Value"],
                                    "https://lschema.org/attributeIndex": [{"@value":"0"}],
                                    "http://annotation": [{"@value":"value"}]
                                }]
                            }
                        ]
                    }
                ]
            }]
        }]
    }


]
