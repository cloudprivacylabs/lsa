[
    {
        "name": "basicSlice",
        "schema": {
            "@context": {
                "ls":"https://lschema.org/"
            },
            "@type":"ls:Schema",
            "@id": "http://testSchema",
            "https://lschema.org/layer": {
                "@type": ["https://lschema.org/Object",
                          "https://lschema.org/valueType"],
                "@id":"root",
                "ls:Object/attributes": [
                    {
                        "@id":  "attr1",
                        "@type": "ls:Value",
                        "http://someKey" : [
                            {"@value": "someValue"}
                        ]
                    },
                    {
                        "@id":  "attr2" ,
                        "@type": "ls:Value",
                        "http://privacy": [
                            {
                                "@value": "flg1"
                            }
                        ]
                    },
                    {
                        "@id":"attr3",
