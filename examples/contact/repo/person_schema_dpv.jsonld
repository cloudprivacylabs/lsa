{
    "@context": "https://layeredschemas.org/ls.json",
    "@id": "http://example.org/Person_dpv/schema",
    "@type": "SchemaVariant",
    "description": [
    "This is the schema variant for a simple Person object with a contact list.",
    "The schema base defines the Person attributes, and the overlay uses the data privacy vocabulary",
    "to mark firstName and lastName as Name and Identifying classigications."
    ],
    "targetType": "http://example.org/Person",
    "schema": "http://example.org/Person/schemaBase",
    "overlays": [ 
        "http://example.org/Person/dpv"
    ]
}
