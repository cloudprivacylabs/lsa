[
  {
    "@id": "http://hl7.org/fhir/DeviceUseStatement/description",
    "@type": [
      "https://lschema.org/Overlay"
    ],
    "https://lschema.org/layer": [
      {
        "@id": "http://hl7.org/fhir/DeviceUseStatement",
        "@type": [
          "https://lschema.org/Object",
          "http://hl7.org/fhir/DeviceUseStatement"
        ],
        "https://lschema.org/Object/attributes": [
          {
            "@id": "http://hl7.org/fhir/DeviceUseStatement#timingDateTime",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "How often the device was used."
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/DeviceUseStatement#recordedOn",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "A date, date-time or partial date (e.g. just year or year + month).  If hours and minutes are specified, a time zone SHALL be populated. The format is a union of the schema types gYear, gYearMonth, date and dateTime. Seconds must be provided due to schema type constraints but may be zero-filled and may be ignored.                 Dates SHALL be valid dates."
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/DeviceUseStatement#implicitRules",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "String of characters used to identify a name or a resource"
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/DeviceUseStatement#language",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "A string which has at least one character and no leading or trailing whitespace and where there is no whitespace other than single spaces in the contents"
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/DeviceUseStatement#status",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "A code representing the patient or other source's judgment about the state of the device used that this statement is about.  Generally this will be active or completed."
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/DeviceUseStatement#resourceType",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "This is a DeviceUseStatement resource"
              }
            ]
          }
        ]
      }
    ],
    "https://lschema.org/targetType": [
      {
        "@id": "http://hl7.org/fhir/DeviceUseStatement"
      }
    ]
  }
]