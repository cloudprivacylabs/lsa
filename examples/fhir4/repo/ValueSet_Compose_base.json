          {
            "@id": "http://hl7.org/fhir/ValueSet_Compose#lockedDate",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/attributeName": [
              {
                "@value": "lockedDate"
              }
            ],
            "https://lschema.org/description": [
              {
                "@value": "A date or partial date (e.g. just year or year + month). There is no time zone. The format is a union of the schema types gYear, gYearMonth and date.  Dates SHALL be valid dates."
              }
            ],
            "https://lschema.org/targetType": [
              {
                "@value": "string"
              }
            ],
            "https://lschema.org/validation#pattern": [
              {
                "@value": "^([0-9]([0-9]([0-9][1-9]|[1-9]0)|[1-9]00)|[1-9]000)(-(0[1-9]|1[0-2])(-(0[1-9]|[1-2][0-9]|3[0-1]))?)?$"
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/ValueSet_Compose#_lockedDate",
            "@type": [
              "https://lschema.org/Reference"
            ],
            "https://lschema.org/Reference#reference": [
              {
                "@id": "http://hl7.org/fhir/Element"
