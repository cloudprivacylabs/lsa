      {
        "@id": "http://hl7.org/fhir/MedicationStatement",
        "@type": [
          "https://lschema.org/Object",
          "http://hl7.org/fhir/MedicationStatement"
        ],
        "https://lschema.org/Object/attributes": [
          {
            "@id": "http://hl7.org/fhir/MedicationStatement#dateAsserted",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "A date, date-time or partial date (e.g. just year or year + month).  If hours and minutes are specified, a time zone SHALL be populated. The format is a union of the schema types gYear, gYearMonth, date and dateTime. Seconds must be provided due to schema type constraints but may be zero-filled and may be ignored.                 Dates SHALL be valid dates."
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/MedicationStatement#implicitRules",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "String of characters used to identify a name or a resource"
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/MedicationStatement#effectiveDateTime",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "The interval of time during which it is being asserted that the patient is/was/will be taking the medication (or was not taking, when the MedicationStatement.taken element is No)."
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/MedicationStatement#resourceType",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "This is a MedicationStatement resource"
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/MedicationStatement#language",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "A string which has at least one character and no leading or trailing whitespace and where there is no whitespace other than single spaces in the contents"
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/MedicationStatement#status",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "A string which has at least one character and no leading or trailing whitespace and where there is no whitespace other than single spaces in the contents"
              }
            ]
          }
        ]
      }
    ],
