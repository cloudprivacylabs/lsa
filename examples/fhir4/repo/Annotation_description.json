[
  {
    "@id": "http://hl7.org/fhir/Annotation/description",
    "@type": [
      "https://lschema.org/Overlay"
    ],
    "https://lschema.org/layer": [
      {
        "@id": "http://hl7.org/fhir/Annotation",
        "@type": [
          "https://lschema.org/Object",
          "http://hl7.org/fhir/Annotation"
        ],
        "https://lschema.org/Object/attributes": [
          {
            "@id": "http://hl7.org/fhir/Annotation#id",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "A sequence of Unicode characters"
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/Annotation#text",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "A string that may contain Github Flavored Markdown syntax for optional processing by a mark down presentation engine"
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/Annotation#time",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "A date, date-time or partial date (e.g. just year or year + month).  If hours and minutes are specified, a time zone SHALL be populated. The format is a union of the schema types gYear, gYearMonth, date and dateTime. Seconds must be provided due to schema type constraints but may be zero-filled and may be ignored.                 Dates SHALL be valid dates."
              }
            ]
          },
          {
            "@id": "http://hl7.org/fhir/Annotation#authorString",
            "@type": [
              "https://lschema.org/Value"
            ],
            "https://lschema.org/description": [
              {
                "@value": "The individual responsible for making the annotation."
              }
            ]
          }
        ]
      }
    ],
    "https://lschema.org/targetType": [
      {
        "@id": "http://hl7.org/fhir/Annotation"
      }
    ]
