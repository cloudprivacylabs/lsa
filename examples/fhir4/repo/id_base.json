[
  {
    "@id": "http://hl7.org/fhir/id/base",
    "@type": [
      "https://lschema.org/Schema"
    ],
    "https://lschema.org/layer": [
      {
        "@id": "http://hl7.org/fhir/id",
        "@type": [
          "https://lschema.org/Value",
          "http://hl7.org/fhir/id"
        ],
        "https://lschema.org/description": [
          {
            "@value": "Any combination of letters, numerals, \"-\" and \".\", with a length limit of 64 characters.  (This might be an integer, an unprefixed OID, UUID or any other identifier pattern that meets these constraints.)  Ids are case-insensitive."
          }
        ],
        "https://lschema.org/targetType": [
          {
            "@id": "string"
          }
        ],
        "https://lschema.org/validation#pattern": [
          {
            "@value": "^[A-Za-z0-9\\-\\.]{1,64}$"
          }
        ]
      }
    ],
    "https://lschema.org/targetType": [
      {
        "@id": "http://hl7.org/fhir/id"
      }
    ]
  }
]